{
    "name": "ziglite",
    "description": "A Laravel route name translator for Javascript",
    "version": "0.1",
    "keywords": [
        "ziglite",
        "laravel",
        "named",
        "routing",
        "routes"
    ],
    "type": "module",
    "author": {
        "name": "Saad Sidqui",
        "email": "galacticinterloper.dev@gmail.com"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/galactic-interloper/ziglite",
    "bugs": "https://github.com/galactic-interloper/ziglite/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/galactic-interloper/ziglite.git"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "eslint": "^8.57.0",
        "microbundle": "^0.15.1",
        "qs": "^6.12.0"
